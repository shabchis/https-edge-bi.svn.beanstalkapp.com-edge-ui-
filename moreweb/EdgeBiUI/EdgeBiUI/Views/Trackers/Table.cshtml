@model List<EdgeBiUI.Models.TrackerRowModel>
@using Easynet.Edge.UI.Data;

<div class="IndexTable">
    <div class="IndexTableHeader">
        <div class="" style="float:left; width:80%; height:14px; overflow:hidden;"><div style="padding-left:30px;">Name</div></div>
        <div class="" style="float:left; width:20%; height:14px; overflow:hidden;">Channel</div>
    </div>
    @foreach (EdgeBiUI.Models.TrackerRowModel tracker in Model)
    {
        <div class="IndexTableRow" id="IndexTableRow_@(tracker.TrackerID)" onclick="selectRow(@(tracker.TrackerID), false);">
            <div class="IndexTableName" style="width:80%;"><div style="padding-left:30px;" onclick="openTrackerDetails(@tracker.Identifier)" title="Gk=@tracker.TrackerID">@tracker.TrackerName (@tracker.Identifier)</div></div>
            <div class="IndexTableDesc" style="width:20%;">@tracker.ChannelName</div>
        </div>
    }

    @if (Model.Count == 0)
    {
        <div class="IndexTableRowNoResult">No Trackers Found</div>
    }
</div>