@model List<EdgeBiUI.Models.AdgroupRowModel>
@using Easynet.Edge.UI.Data;


    @foreach (EdgeBiUI.Models.AdgroupRowModel adgroup in Model)
    {
        <div class="IndexTableRow" id="IndexTableRow_@(adgroup.AdgroupGK)" onclick="selectRow(@(adgroup.AdgroupGK), false);">
            <div class="adgroupMultiSelect" style="float:left; margin-left:50px;">
                    <input type="checkbox" name="adgroupMultiSelect_@(adgroup.AdgroupGK)" />
            </div>
            <div class="IndexTableName">
                <div style="margin-left:20px; padding-left:20px; background-image:url('@Url.Content("~/Content/images/bullet.png")'); background-repeat:no-repeat;" onclick="openAdgroupDetails(@adgroup.AdgroupGK)" title="GK=@adgroup.AdgroupGK">
                    @adgroup.AdgroupName
                </div>
            </div>
        </div>
    }